\documentclass{minimal}
\input{regression-test}

\RequirePackage{pgf}

\newsavebox\testbox
\newcommand\test[1]{%
  \savebox\testbox{%
    \begin{pgfpicture}
      #1
      \pgfusepathqstroke
    \end{pgfpicture}
  }%
  \begingroup
    \showboxbreadth\maxdimen
    \showboxdepth\maxdimen
    \showbox\testbox
  \endgroup
}

\START

\TEST{\pgfpathmoveto}
  {%
    \test{\pgfpathmoveto{\pgfpoint{1cm}{1cm}}}%
  }

\TEST{\pgfpathlineto}
  {%
    \test{\pgfpathlineto{\pgfpoint{1cm}{1cm}}}%
  }

\END
